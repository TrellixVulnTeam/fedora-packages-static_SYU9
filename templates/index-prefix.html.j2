{% extends "layout.html.j2" %}
{% set link_prefix = "../" %}
{% set no_index = True %}

{% if main_is_static %}
  {% set back_page = "index.html" %}
{% else %}
  {% set back_page = "index-static.html" %}
{% endif %}

{% block title %}"{{ prefix }}" Index - Fedora Packages{% endblock %}

{% block content %}
  <h3 class="text-center">Packages that start with "{{ prefix }}".</h3>

  <p class="text-center">
    <a href="{{ link_prefix }}{{ back_page }}">Back to the packages index &crarr;</a>
  </p>

  <ul>
  {% for package in packages %}
    <li><a href="../pkgs/{{ package[0] }}/{{ package[1] }}/index.html">{{ package[1] }}</a></li>
  {% endfor %}
  </ul>
{% endblock %}
